{
  "name": "monitrax",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:validation": "npm run seed:validation && vitest run --reporter=verbose",
    "test:regression": "vitest run tests/regression",
    "test:sanity": "vitest run tests/sanity",
    "test:calculations": "vitest run tests/calculations",
    "seed:validation": "npx ts-node prisma/seed-validation.ts",
    "validate": "npm run seed:validation && npm run test && npm run generate:report",
    "generate:report": "npx ts-node tests/validation/generate-report.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.553.0",
    "next": "15.1.4",
    "next-themes": "^0.4.6",
    "openai": "^6.9.1",
    "postcss": "^8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.5.0",
    "resend": "^6.5.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8",
    "eslint-config-next": "15.1.4",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "vitest": "^1.6.1"
  }
